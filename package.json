{
  "name": "datamatic",
  "description": "RxJS + JSON-Schema (Ajv) Based Observable and Validating Data Models and Pipelines",
  "homepage": "https://webfreshener.github.io/datamatic",
  "author": "Van Schroeder <van@webfreshener.com>",
  "version": "0.9.13",
  "main": "./dist/datamatic.node.js",
  "module": "./dist/datamatic.umd.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Webfreshener/datamatic.git"
  },
  "scripts": {
    "test": "jest",
    "clear-tests": "jest --clearCache",
    "cover": "jest --coverage && codacy-cli analyze --config .codacy/codacy.yaml",
    "build-test": "webpack --mode development; npm test",
    "build": "webpack --mode production",
    "docs": "./node_modules/.bin/esdoc",
    "report": "$HOME/Library/Caches/Codacy/coverage-reporter/11.9.2/codacy-coverage-reporter report",
    "release": "bash scripts/release.sh",
    "demo:window-html": "open integration/window-html/index.html",
    "demo:umd-react": "npm --prefix integration/umd-react run dev",
    "demo:umd-angular": "npm --prefix integration/umd-angular start",
    "demo:commonjs": "node integration/commonjs-node/index.js"
  },
  "engines": {
    "node": "24.13.0"
  },
  "files": [
    "index.js",
    "dist/*"
  ],
  "keywords": [
    "rxjs",
    "ajv",
    "json-schema",
    "validation",
    "validatable",
    "observable",
    "proxy",
    "model",
    "json",
    "data",
    "object",
    "array",
    "hash",
    "filter",
    "map",
    "dictionary"
  ],
  "devDependencies": {
    "@babel/cli": "^7.28.6",
    "@babel/core": "^7.28.6",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@babel/preset-env": "^7.28.6",
    "@babel/preset-react": "^7.28.5",
    "@babel/runtime": "^7.28.6",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "babel-loader": "^10.0.0",
    "chai": "^6.2.2",
    "deep-equal": "^2.2.3",
    "es6-weak-map": "^2.0.3",
    "esdoc": "^1.1.0",
    "esdoc-accessor-plugin": "^1.0.0",
    "esdoc-exclude-source-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "esdoc2": "^2.1.5",
    "jest": "^30.2.0",
    "jest-babel-istanbul": "^0.1.0",
    "jest-each": "^30.2.0",
    "jshint": "^2.13.6",
    "json-schema-migrate": "^2.0.0",
    "jsonpath": "^1.1.1",
    "lodash.merge": "^4.6.2",
    "mocha": "^11.7.5",
    "mocha-lcov-reporter": "^1.3.0",
    "object.assign": "^4.1.7",
    "rxjs": "^7.8.2",
    "rxjs-compat": "^6.6.7",
    "weakmap": "^0.0.6",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1",
    "webpack-rxjs-externals": "^2.0.0"
  },
  "jest": {
    "collectCoverage": true,
    "testPathIgnorePatterns": [
      "/integration/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/integration/"
    ]
  }
}
